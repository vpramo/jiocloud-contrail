[DEFAULTS]
host_ip = <%= @contrail_ip %>
collectors = <%= @collector_ip %>:8086
http_server_port = 8090
rest_api_port = 8081
rest_api_ip = 0.0.0.0
log_file_size = <%= @log_file_size %>
log_level = <%= @log_level %>
log_local = <%= @log_local %>
log_category =
log_file = /var/log/contrail/contrail-analytics-api.log
multi_tenancy = false
aaa_mode = no-auth

[DISCOVERY]
disc_server_ip = <%= @discovery_virtual_ip %>
disc_server_port = 5998

[REDIS]
redis_server_port = 6379
redis_query_port = 6379
